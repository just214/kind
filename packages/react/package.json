{
  "name": "react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/react.cjs",
      "import": "./dist/react.es.js"
    }
  },
  "main": "dist/react.cjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "typecheck": "tsc --noEmit",
    "build": "vite build",
    "build:css": "tailwindcss -m -i ./src/tailwind-entry.css -o ./dist/styles.css",
    "storybook": "concurrently \"npm run storybook:css\" \"sb dev -p 6006\"",
    "storybook:css": "tailwindcss -w -i ./src/tailwind-entry.css -o ./src/index.css",
    "build-storybook": "concurrently \"npm run build-storybook:css\" \"storybook build\"",
    "build-storybook:css": "tailwindcss -m -i ./src/tailwind-entry.css -o ./src/index.css",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@headlessui/react": "^1.7.14",
    "@just214/eslint-config": "*",
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/addon-interactions": "^7.0.9",
    "@storybook/addon-links": "^7.0.9",
    "@storybook/blocks": "^7.0.9",
    "@storybook/cli": "^7.0.9",
    "@storybook/react": "^7.0.9",
    "@storybook/react-vite": "^7.0.9",
    "@storybook/testing-library": "^0.1.0",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-dts": "^2.3.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "peerDependencies": {
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "@headlessui/react": "^1.7.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/just214/kind.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@just214"
  }
